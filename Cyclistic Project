--CREATING VIEW FOR VISUALISATION LATER ON

CREATE VIEW ride_data AS
SELECT 
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.[tripdata202101/02]
UNION
SELECT 
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202103
UNION
SELECT 
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202104
UNION
SELECT 
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202105
UNION
SELECT 
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202106
UNION
SELECT
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202107
UNION
SELECT
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202108
UNION
SELECT 
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202109
UNION
SELECT
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202110
UNION
SELECT 
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202111
UNION
SELECT 
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202112
UNION
SELECT
rideable_type,
member_casual,
day_of_week
FROM 
PortfolioProject.dbo.tripdata202201
